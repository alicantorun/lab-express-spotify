<table class="tracks-wrapper">
    <tr class="heading">
        <th>Title</th>
        <th>Listen</th>

    </tr>
    {{#each items}}
    <tr class="tracks">
        <td>
            <p>{{name}}</p>
        </td>
        <td>
            <audio src="{{preview_url}}" controls="true" volume="true" class="listen"></audio>
        </td>
    </tr>
    {{/each}}

</table>